<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.techzhai.model.UserBean" table="t_user">
		<id name="u_id" column="u_id">
			<generator class="native"></generator>
		</id>
		<property name="u_nickname" column="u_nickname"></property>
		<property name="userpwd" column="userpwd"></property>
		<property name="u_email" column="u_email"></property>
		<property name="edu_background" column="edu_background"></property>
		<property name="blacklist" column="blacklist"></property>
	</class>
</hibernate-mapping>