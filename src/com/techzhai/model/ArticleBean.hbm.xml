<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.techzhai.model.ArticleBean" table="t_article">
		<id name="w_id" column="w_id">
			<generator class="native"></generator>
		</id>
		<property name="w_title" column="w_title"></property>
		<property name="w_content" column="w_content"></property>
		<property name="w_readnum" column="w_readnum"></property>
		<property name="w_publishtime" column="w_publishtime"></property>
		<property name="w_publisher" column="w_publisher"></property>
		<property name="w_articletype" column="w_articletype"></property>
	</class>
</hibernate-mapping>